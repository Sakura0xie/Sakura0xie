#define _CRT_SECURE_NO_WARNINGS 1

#include<stdio.h>

int main()
{	
	printf("奇偶判断");
	int num1 = 0;
	scanf("%d", &num1);
	int num2 = num1 % 2;
	if (num2 == 0)
		printf("这个数是偶数");
	else
		printf("这个数是奇数");
	int num3 = 0;
	while (num3 <= 100)
	{
		int num4 = num3 % 2;
		if (num4 == 1)
		{
			printf("%d ", num3);
		};
		num3++;
	};
	return 0;

}
